    <style name="textcolorWhiteToWhite">
        <item name="android:textColor">@color/white</item>
        <item name="android:indeterminateTint">@color/Szarawy_ciemny</item>
    </style>

    <style name="buttontextcolor">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="background_list_menu">
        <item name="background">@drawable/border_list_diet</item>
    </style>

    <style name="PersonalDataEdit">
